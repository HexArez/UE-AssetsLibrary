{
  "name": "ue-library",
  "version": "1.0.0",
  "description": "Asset library",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . UELibrary --platform=win32 --arch=x64 --out=dist --overwrite",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "keywords": [],
  "author": "HexArez",
  "license": "ISC",
  "dependencies": {
    "lowdb": "^7.0.1"
  },
  "build": {
    "appId": "com.darkreign.uelibrary",
    "productName": "UE Library",
    "files": [
      "**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis"
    }
  }
}
